BOT_TOKEN=put_your_token_here
ADMIN_IDS=123456789,987654321
DB_URL=sqlite:///bot.db
LOG_LEVEL=INFO
TZ=Asia/Aqtobe

# Screenshot settings
# Note: For Linux servers, if screenshots are white, install dependencies:
# apt-get install -y libglib2.0-0 libnss3 libnspr4 libatk1.0-0 libatk-bridge2.0-0
SCREENSHOT_HEADLESS=true
SCREENSHOT_TIMEOUT=15000
SCREENSHOT_WAIT_SELECTOR=header
SCREENSHOT_FALLBACK_SELECTOR=main
SCREENSHOT_DARK_THEME=true

# Encryption (32-byte Base64 urlsafe key for Fernet)
ENCRYPTION_KEY=generate_with_python_or_openssl

# Instagram / Authorization
IG_HEADLESS=true
IG_LOGIN_TIMEOUT=25000
IG_2FA_TIMEOUT=300000
IG_MINI_APP_URL=https://yourdomain.com/telegram_mini_app/index.html

# Instagram Screenshot (optional, can reuse SCREENSHOT_*)
IG_SCREENSHOT_HEADER_SELECTOR=header
IG_SCREENSHOT_FALLBACK_SELECTOR=main

# RapidAPI settings
RAPIDAPI_HOST=instagram120.p.rapidapi.com
RAPIDAPI_URL=https://instagram120.p.rapidapi.com/api/instagram/profile
API_DAILY_LIMIT=950
RAPIDAPI_TIMEOUT_SECONDS=10

# Auto-check settings
AUTO_CHECK_INTERVAL_MINUTES=15